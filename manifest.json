{
  "manifest_version": 2,
  "name": "Memory Game",
  "version": "0.1",
  "background": {
	  "scripts": ["background.js"]
	},
  
  "content_scripts": [
	  {
		"matches": [
		  "<all_urls>"
		],
		"js": ["js/jquery-3.4.1.min.js","content.js"]
	  }
	],
	
	"browser_action": {
		"default_icon": "aces.png",
		"default_popup": "main_screen.html"
	}
}